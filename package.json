{
  "name": "infra-repo",
  "version": "0.1.10",
  "bin": {
    "infra-repo": "bin/infra-repo.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:fga": "jest ./*/fineGrained.test.ts --passWithNoTests",
    "test:snap": "jest ./*/snapshot.test.ts --passWithNoTests",
    "cdk": "cdk",
    "prepare": "husky",
    "format": "prettier --write '*/**/*.ts'",
    "format:ci": "prettier --check '*/**/*.ts'",
    "lint": "eslint '*/**/*.ts' --max-warnings 0 --fix",
    "lint:ci": "eslint '*/**/*.ts' --max-warnings 0",
    "lint-fix": "prettier --write '*/**/*.ts' && eslint '*/**/*.ts' --max-warnings 0",
    "md-lint-fix": "prettier --write '*/**/*.md' && markdownlint-cli2 '*/**/*.md'",
    "release": "standard-version && git push --follow-tags",
    "depcheck": "depcheck"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "markdownlint-cli2": "^0.13.0",
    "prettier": "^3.3.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.5"
  },
  "dependencies": {
    "esbuild": "^0.23.1",
    "husky": "^9.1.5",
    "typescript": "~5.5.4",
    "ts-node": "^10.9.2",
    "aws-cdk": "2.155.0",
    "aws-cdk-lib": "2.155.0",
    "cdk-nag": "^2.28.195",
    "winston": "^3.14.2",
    "constructs": "^10.3.0",
    "@aws-cdk/aws-kinesisfirehose-alpha": "^2.155.0-alpha.0",
    "@aws-cdk/aws-kinesisfirehose-destinations-alpha": "^2.155.0-alpha.0",
    "@aws-lambda-powertools/tracer": "^1.18.1",
    "@aws-sdk/client-cloudwatch": "^3.637.0",
    "@aws-sdk/client-dynamodb": "^3.637.0",
    "@aws-sdk/client-kinesis": "^3.637.0",
    "@aws-sdk/lib-dynamodb": "^3.637.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --max-warnings 0 --fix"
    ],
    "*.md": [
      "prettier --write",
      "markdownlint-cli2"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
